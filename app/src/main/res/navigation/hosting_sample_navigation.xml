<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/hosting_sample_navigation"
    app:startDestination="@id/sampleFragment">

    <fragment
        android:id="@+id/sampleFragment"
        android:name="com.bryanherbst.fragmentstate.SampleFragment"
        android:label="SampleFragment"
        tools:layout="@layout/fragment_sample">
        <action
            android:id="@+id/action_sampleFragment_to_stubSampleFragment"
            app:destination="@id/stubSampleFragment" />
    </fragment>
    <fragment
        android:id="@+id/stubSampleFragment"
        android:name="com.bryanherbst.fragmentstate.StubSampleFragment"
        android:label="StubSampleFragment"
        tools:layout="@layout/stub_fragment"/>


</navigation>